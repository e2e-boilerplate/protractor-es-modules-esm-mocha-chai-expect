    "es6",
    "esm",
    "mocha",
    "chai",
    "boilerplate",
    "example",
    "e2e",
    "e2e test"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "jasmine": "3.5.0",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "protractor": "5.4.3"
  },
  "husky": {
    "hooks": {
