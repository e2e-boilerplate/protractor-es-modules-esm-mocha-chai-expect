{
  "name": "protractor-es6-modules-esm-mocha-chai",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate, using ES6-modules, ESM, Mocha and Chai",
  "scripts": {
    "lint": "npx eslint --ext .js ./ --fix",
    "upadte-webderiver": "npx webdriver-manager update",
    "test": "npx protractor -r esm protractor.conf.js",
    "test-ci": "npx protractor -r esm protractor-ci.conf.js"
  },
  "keywords": [
    "protractor",
    "es6 modules",
    "esm",
    "mocha",
    "chai",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "esm": "3.2.25",
    "husky": "3.0.9",
    "jasmine": "3.5.0",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.0",
    "protractor": "5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "puppeteer": "2.0.0"
  }
}
